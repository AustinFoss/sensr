{
  "name": "js-p2phttp-browser",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"astro dev --host 0.0.0.0\" \"pnpm build:libp2p-http-sw\" \"pnpm build:helios-ww\"",
    "build": "astro build",
    "build:libp2p-http-sw": "cd src/workers/libp2p-http-sw/ && pnpm build -o ../../../public/ && while inotifywait -e close_write ./src/*; do pnpm build -o ../../../public/; done",
    "build:helios-ww": "cd src/workers/helios-ww/ && pnpm build --outDir ../../../public/workers && while inotifywait -e close_write ./src/*; do pnpm build --outDir ../../../public/workers; done",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@a16z/helios": "^0.9.0",
    "@astrojs/solid-js": "^5.0.7",
    "@chainsafe/libp2p-noise": "^16.1.0",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@libp2p/bootstrap": "^11.0.43",
    "@libp2p/circuit-relay-v2": "^3.2.12",
    "@libp2p/crypto": "^5.1.6",
    "@libp2p/identify": "^3.0.28",
    "@libp2p/interface": "^2.10.5",
    "@libp2p/interface-internal": "^2.3.18",
    "@libp2p/mplex": "^11.0.32",
    "@libp2p/peer-id": "^5.1.4",
    "@libp2p/ping": "^2.0.28",
    "@libp2p/webrtc": "^5.2.10",
    "@libp2p/webtransport": "^5.0.37",
    "@multiformats/multiaddr": "^12.4.0",
    "@nanostores/solid": "^1.0.0",
    "astro": "^5.5.6",
    "cborg": "^4.2.10",
    "ethers": "^6.14.4",
    "it-byte-stream": "^2.0.3",
    "it-length-prefixed-stream": "^2.0.3",
    "libp2p": "^2.8.2",
    "main-event": "^1.0.1",
    "nanostores": "^1.0.1",
    "solid-js": "^1.9.5",
    "viem": "^2.31.4",
    "workbox-routing": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}